@{
    ViewBag.Title = "Our Works";
}

@section metatags{
    <meta name="description" content="Browse our rich library of publications and projects.">
    <meta name="keywords" content="economics, diesel, oil, generation, ghana, power">
}

<!--Title Strip-->
<section id="content-region-3" class="page-tree-bg">
    <div class="container text-center">
        <p style="color: white;">Our Works</p>
    </div>
</section>
<!--End of Title Strip-->

<!--preloader-->
<div id="preloader"></div>
<!--preloader-->

<div class="space-70"></div>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <ul class="portfolio-filters">
                <li class="filter active" data-filter="all">all</li>
                <li class="filter" data-filter="publications">Publications</li>
            </ul>

            <div id="grid" class="row">
                
                <div class=" mix publications col-md-4 col-sm-6 margin-btm-40">
                    <div class="portfolio-sec">
                        <div class="portfolio-thumnail">
                            <a href="Porfolio/Detail">
                                <img src="~/Content/Images/Publications/pub-1.jpg" />
                            </a>
                        </div>
                        <div class="portfolio-desc text-center">
                            <h4 class="portfolio-post-title">Economic Assessment of Utilizing Diesel Derived From Used Lubricating Oil for Power Generation in Ghana</h4>
                            <span class="portfolio-post-cat">Renewable Energy</span>
                            <h4>@Html.ActionLink("More Detail", "Detail", "Portfolio", null, new { @class = "btn theme-btn-default btn-lg" })</h4>
                        </div>
                    </div>
                </div><!--portfolio item -->

            </div><!--grid-->
        </div>
    </div><!--row-->
</div><!--portfolio container end-->

<div class="space-70"></div>




